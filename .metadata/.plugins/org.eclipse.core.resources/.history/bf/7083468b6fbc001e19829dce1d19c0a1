		aminoTable.put('Q', 128.05858);
		aminoTable.put('G', 57.02146);
		aminoTable.put('H', 137.05891);
		aminoTable.put('I', 113.08406);
		aminoTable.put('L', 113.08406);
		aminoTable.put('K', 128.09496);
		aminoTable.put('M', 131.04049);
		aminoTable.put('O', 114.07931);
		aminoTable.put('F', 147.06841);
		aminoTable.put('P', 97.05276);
		aminoTable.put('S', 87.03203);
		aminoTable.put('T', 101.04768);
		aminoTable.put('W', 186.07931);
		aminoTable.put('Y', 163.06333);
		aminoTable.put('V', 99.06841);
		aminoTable.put('t', 181.02745);
